                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select UNT_ID as [Master Fb Id], CA_LINE_NUM as [CA Line Number]
from DW.CA_ELEM a WITH (NOLOCK)
 inner join [HPowners] b on a.unt_id like b.ownerlike
where UpdateDate &gt; DATEADD(hh, -17,DATEADD(dd, 0, DATEDIFF(dd, 0, GETDATE())))
union
select unt_id,CA_LINE_NUM as [CA Line Number]
from dw.ca_elem a with(nolock)
inner join  hpstage.dbo.deletefbid2 b with(nolock) on b.fb_id=a.UNT_ID</property>
